  "dependencies": {
    "c2-event-handler": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "browserify": "^16.2.3",
    "acorn": "^6.0.0",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
